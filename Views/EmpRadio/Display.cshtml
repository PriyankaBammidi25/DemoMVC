            @*<td>@Html.TextBoxFor(a => a.ENAME)</td>*@
            <td>@Html.TextBox("ENAME", (object)ViewBag.E.ENAME)</td>
        </tr>
        <tr>
            <td>Job</td>
            @*<td>@Html.TextBoxFor(a => a.JOB)</td>*@
            <td>@Html.TextBox("JOB", (object)ViewBag.E.JOB)</td>
        </tr>
        <tr>
            <td>Manager</td>
            @*<td>@Html.TextBoxFor(a => a.MGR)</td>*@
            <td>@Html.TextBox("MGR", (object)ViewBag.E.MGR)</td>
        </tr>
        <tr>
            <td>Hiredate</td>
            @*<td>@Html.TextBoxFor(a => a.HIREDATE)</td>*@
            <td>@Html.TextBox("HIREDATE", (object)ViewBag.E.HIREDATE)</td>
        </tr>
