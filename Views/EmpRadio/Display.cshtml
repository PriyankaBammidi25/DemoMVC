            <td>Salary</td>
            @*<td>@Html.TextBoxFor(a => a.SAL)</td>*@
            <td>@Html.TextBox("SAL", (object)ViewBag.E.SAL)</td>
        </tr>
        <tr>
            <td>Commission</td>
            @*<td>@Html.TextBoxFor(a => a.COMM)</td>*@
            <td>@Html.TextBox("COMM", (object)ViewBag.E.COMM)</td>
        </tr>
        <tr>
