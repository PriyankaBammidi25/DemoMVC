@model DemoMVC.Models.EMPDATA
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@using (Html.BeginForm("Update", "Update"))
{
    <table>
        <tr>
            <td>Empno</td>
            <td>
                @Html.TextBoxFor(a => a.EMPNO, Request["Eno"], @*Request.Form["Eno"]*@
                new { onblur = "func();" })
            </td>
        </tr>
        <tr>
            <td>Ename</td>
            <td>@Html.TextBoxFor(a => a.ENAME)</td>
        </tr>
        <tr>
            <td>Job</td>
            <td>@Html.TextBoxFor(a => a.JOB)</td>
        </tr>
        <tr>
            <td>MGR</td>
            <td>@Html.TextBoxFor(a => a.MGR)</td>
        </tr>
        <tr>
            <td>HireDate</td>
            <td>@Html.TextBoxFor(a => a.HIREDATE)</td>
        </tr>
        <tr>
            <td>Salary</td>
            <td>@Html.TextBoxFor(a => a.SAL)</td>
        </tr>
        <tr>
            <td>Commission</td>
            <td>@Html.TextBoxFor(a => a.COMM)</td>
        </tr>
        <tr>
            <td>Deptno</td>
            <td>@Html.TextBoxFor(a => a.DEPTNO)</td>
        </tr>
        <tr>
            <td><input type="submit" value="Update" /></td>
        </tr>
    </table>
    if (ViewBag.msg != null)
    {
        @ViewBag.msg;
    }

}

<script>
    function func() {
        var eno = document.getElementById("EMPNO").value;
        window.location.href = "/Update/GetData?Eno=" + eno;
    }
</script>
